{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "compile cef api",
            "command": "gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-c",
                "-g",
                "${workspaceFolder}\\cefapi\\main_win.c",
                "-o",
                "${workspaceFolder}\\cefapi\\cefapi.o",
                "-L${workspaceFolder}\\Release",
                "-lcef",
                "-I${workspaceFolder}",
                "-IC:\\msys64\\mingw64\\include\\c++\\13.2.0",
                "-IC:\\msys64\\mingw64\\include\\c++\\13.2.0\\x86_64-w64-mingw32\\bits"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        },
        {
            "label": "static cef api library",
            "command": "ar",
            "args": ["rcs","libcefapi.a","cefapi.o"],
            "options": {
              "cwd": "${workspaceFolder}/cefapi"
            },
            "dependsOn": ["compile cef api"]
          },
          {
            "type": "cppbuild",
            "label": "generate c program",
            "command": "gcc.exe",
            "args": [
                "-D windowsapp=1",
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}\\cefapi\\main_win.c",
                "-o",
                "${workspaceFolder}\\Release\\windowshelper.exe",
                "-L${workspaceFolder}\\Release",
                "-lcef",
                "-I${workspaceFolder}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}